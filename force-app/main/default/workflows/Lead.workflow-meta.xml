    <rules>
        <fullName>Mark Demo Request as MQL %28Lead%29</fullName>
        <actions>
            <name>Lifecycle_Stage_Change</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>(1 OR 2 OR 8) AND 3 AND 4 AND 5 AND 6 AND 7</booleanFilter>
        <criteriaItems>
            <field>Lead.Request_a_Demo__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.Request_a_Demo_Yes_No__c</field>
            <operation>equals</operation>
            <value>Yes</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.Date_Moved_to_MQL__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.Date_Moved_to_SAL__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.Date_Moved_to_SQL__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.Recent_Conversion__c</field>
            <operation>contains</operation>
            <value>Sports</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.Recent_Conversion__c</field>
            <operation>contains</operation>
            <value>Demo</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.Request_a_Demo_Demo_Request_Form__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
