    <rules>
        <fullName>New Lead Workflow</fullName>
        <actions>
            <name>New_Lead_Email_Alert</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <description>Email the new owner (or queue members) when a new lead is created or the owner changes (and the current user didn&apos;t assign the lead to themselves).</description>
        <formula>AND(
	OR(
	   ISCHANGED( OwnerId ),
	   ISNEW()
	), 
	OwnerId &lt;&gt; $User.Id,
	OwnerId &lt;&gt; &quot;005U00000070Iup&quot; 
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
