    <rules>
        <fullName>MQL- follow up within 24 hours</fullName>
        <actions>
            <name>MQL_Received</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <booleanFilter>1</booleanFilter>
        <criteriaItems>
            <field>Lead.Status</field>
            <operation>equals</operation>
