<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Custom</fullName>
    <active>false</active>
    <errorConditionFormula>AND( ISPICKVAL(Type__c,&quot;Custom&quot;),ISBLANK(  Custom_Description__c  ))</errorConditionFormula>
    <errorDisplayField>Custom_Description__c</errorDisplayField>
    <errorMessage>Enter Custom Description</errorMessage>
</ValidationRule>
