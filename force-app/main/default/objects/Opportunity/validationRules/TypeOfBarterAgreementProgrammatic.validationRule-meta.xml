<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TypeOfBarterAgreementProgrammatic</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
RecordType.Name == &apos;Advertising&apos;,
TEXT( Channel__c ) == &apos;Programmatic&apos;,
TEXT(Type_of_Agreement__c )==&apos;Net&apos;
)</errorConditionFormula>
    <errorDisplayField>Type_of_Agreement__c</errorDisplayField>
    <errorMessage>The Type of Barter Agreement can&apos;t be Net if Channel is Programmatic</errorMessage>
</ValidationRule>
