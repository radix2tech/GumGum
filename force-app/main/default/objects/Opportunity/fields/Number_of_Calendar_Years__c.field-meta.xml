<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Number_of_Calendar_Years__c</fullName>
    <description>Calculates the opportunity&apos;s number of calendar years</description>
    <externalId>false</externalId>
    <formula>(ABS(YEAR(End_Date2__c)-YEAR(Start_Date2__c)) + 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Number of Calendar Years</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
