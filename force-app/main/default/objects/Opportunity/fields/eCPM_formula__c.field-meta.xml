<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>eCPM_formula__c</fullName>
    <description>This field calculates the CPM Amount divided by the quantity of impressions</description>
    <externalId>false</externalId>
    <formula>If( 
   CPM_Amount__c &gt; 0,
   CPM_Amount__c / (CPM_Quantity__c / 1000),
   0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>eCPM Formula</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
