<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Campaign_Start_Quarter__c</fullName>
    <externalId>false</externalId>
    <formula>TEXT(YEAR(Start_Date2__c)) + &quot;-&quot; +
IF(MONTH(Start_Date2__c) &lt;= 3, &quot;Q1&quot;, 
IF(MONTH(Start_Date2__c) &lt;= 6, &quot;Q2&quot;, 
IF(MONTH(Start_Date2__c) &lt;= 9, &quot;Q3&quot;, 
&quot;Q4&quot;)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Campaign Start Quarter</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
